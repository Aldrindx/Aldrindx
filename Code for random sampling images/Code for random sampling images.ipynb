    "import os\n",
    "import shutil\n",
    "import random\n",
    "import pandas as pd\n",
    "  \n",
    "df = pd.DataFrame()\n",
    "w =0\n",
    "li=[]\n",
    "while w < 500:\n",
    "    w =w+1\n",
    "    apple = random.choice(os.listdir(r\"dataset/\"))\n",
    "    o = ''.join([\"dataset/\", apple])\n",
    "    #print(apple)\n",
    "    shutil.move(o,\"new folder")\n # create a new folder", 
    "    li.append(apple)\n",
    "    \n",
    "    #print(list)\n",
    "df['images']=li\n",
    "df.to_csv(r'img.csv',index=False)"
   ]
